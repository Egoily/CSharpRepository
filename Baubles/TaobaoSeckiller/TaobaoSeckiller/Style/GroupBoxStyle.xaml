        <Setter Property="Margin" Value="3"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="BorderBrush" Value="{StaticResource AccentBlueColor}"/>
    </Style>
</ResourceDictionary>